{
  "schemaVersion": 1,
  "id": "structure_saplings",
  "version": "${version}",
  "name": "Structure Saplings",
  "description": "A server-side, vanilla-friendly, multiplatform, lightweight mod which makes saplings grow into a specific structure!",
  "authors": [
    "Oak Platinum"
  ],
  "contact": {
    "homepage": "https://mln.hz6826.com.cn/",
    "sources": "https://github.com/OakPlatinum/structure_saplings"
  },
  "license": "MIT",
  "icon": "assets/structure_saplings/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.hz6826.structure_saplings.fabric.StructureSaplings"
    ],
    "client": [
      "com.hz6826.structure_saplings.fabric.client.ExampleModFabricClient"
    ]
  },
  "mixins": [
    "structure_saplings.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.9",
    "minecraft": "~1.21",
    "java": ">=21",
    "architectury": ">=13.0.8",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
